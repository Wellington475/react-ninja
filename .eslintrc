{
  "env" : {
    "node" : true,
    "es6" : true
  },

  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "spread" : true,
      "restParams" : true
    }
  },
  "rules": {
    "eol-last": 0
  }
}